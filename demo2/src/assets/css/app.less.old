.app-sidenav {
  &-container {
    /* When the sidenav is not fixed, stretch the sidenav container to fill the available space. This
      causes `<mat-sidenav-content>` to act as our scrolling element for desktop layouts. */
    flex: 1;
    min-height: 300px;
    height: 100%;

    &-panel {
      background: -moz-linear-gradient(left, rgba(194,24,92,1) 1%, rgba(137,68,83,1) 56%, rgba(194,22,94,1) 100%); /* FF3.6-15 */
      background: -webkit-linear-gradient(left, rgba(194,24,92,1) 1%,rgba(137,68,83,1) 56%,rgba(194,22,94,1) 100%); /* Chrome10-25,Safari5.1-6 */
      background: linear-gradient(to right, rgba(194,24,92,1) 1%,rgba(137,68,83,1) 56%,rgba(194,22,94,1) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
      filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c2185c', endColorstr='#c2165e',GradientType=1 ); /* IE6-9 */
    }
    &-item {
      font-size: 12px !important;
      color: #f0f0f0;
      // background-color: #673AB7;
    }
    &-item:active {
      color: #ffff00;
    }
  }
}

