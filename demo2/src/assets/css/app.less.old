.app-container {
  display: flex;
  flex-direction: column;
}

.app-container > * {
  width: 100%;
}

.app-container form > * {
  margin: 0px;
}

.app-section {
    display: flex;
    flex-direction: row;
    width: 100%;
}

section > div{
  box-shadow: 0 3px 1px -2px rgba(0,0,0,.2), 0 2px 2px 0 rgba(0,0,0,.14), 0 1px 5px 0 rgba(0,0,0,.12);  
  padding: 20px;
  margin-top: 20px;
  margin-left: 20px;
  margin-bottom: 20px;
  margin-right: 20px;
}

.app-section > div:nth-child(1){
  flex: 1;
}
.app-section > div:nth-child(2){
  flex: 1;
}

/* gradient generated from http://www.colorzilla.com/gradient-editor/ */
.app-section-column {
    background-color:#ffffff;
    /* padding-left: 10px;*/
}

.app-section-column > * {
