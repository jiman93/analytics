mat-hint.SELECTED {
  &-YES {
    color: #5520B2;
    font-weight: 800;
  }
  &-NO {
    color: #c0c0c0;
    font-weight: 100;
  }
}

mat-hint.UNSELECTED {
  &-YES {
    color: #c0c0c0;
    font-weight: 100;
  }
  &-NO {
    color: #5520B2;
    font-weight: 800;
  }
}

mat-radio-group.CHANGED, mat-select.CHANGED, input.CHANGED {
  /* border: 1px solid #d66; */
  box-shadow: 0px 0px 10px 1px #5520B2;
  padding: 5px;
}

